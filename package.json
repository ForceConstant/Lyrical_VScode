{
  "name": "lyrical",
  "publisher": "ForceConstant",
  "displayName": "Lyrical",
  "description": "VScode extension to calculate syllables, find rhymes and more",
  "repository": {
    "type": "git",
    "url": "https://github.com/ForceConstant/Lyrical_VScode.git"
  },
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lyrical.showRhymes",
        "title": "Lyrical: Show Rhymes"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/syllable": "^5.0.2",
    "@types/vscode": "^1.101.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "rhymes": "^2.0.0",
    "syllable": "^5.0.1"
  }
}
